@font-face {
  font-family: 'eruda-icon';
  src: {{{src}}};
}

[class^='icon-'],
[class*=' icon-'] {
  font-family: 'eruda-icon' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
}
{{/each}}